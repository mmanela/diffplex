@model DiffPlex.DiffBuilder.Model.SideBySideDiffModel
@*
    For more information on enabling MVC for empty projects, visit http://go.microsoft.com/fwlink/?LinkID=397860
*@
<div id="diffBox">
    <div id="leftPane">
        <div class="diffHeader">Old Text</div>
        @(await Html.PartialAsync("DiffPane", Model.OldText))
    </div>
    <div id="rightPane">
        <div class="diffHeader">New Text</div>
        @(await Html.PartialAsync("DiffPane", Model.NewText))
    </div>
    <div class="clear">
    </div>
</div>

<script type="text/javascript">

        $(function() {
            InitializeDiffPanes();
        });
</script>
