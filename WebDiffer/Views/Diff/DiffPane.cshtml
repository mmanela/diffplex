@model DiffPlex.DiffBuilder.Model.DiffPaneModel
<div class="diffPane">
    <table cellpadding="0" cellspacing="0" class="diffTable">
        @foreach (var diffLine in Model.Lines)
        {
        <tr>
            <td class="lineNumber">
                @if (diffLine.Position.HasValue)
                {
                    @diffLine.Position
                }
                else
                {
                    <text>&nbsp;</text>
                }
            </td>
            <td class="line @diffLine.Type.ToString() Line">
                <span class="lineText">
                    @await Html.PartialAsync("DiffLine", diffLine)
                </span>
            </td>
        </tr>
        }
    </table>
</div>